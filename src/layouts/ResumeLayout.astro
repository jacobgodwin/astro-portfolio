---
import '../styles/globals.css'
import BaseLayout from './BaseLayout.astro'
type Props = {
  frontmatter: {
    title: string
  }
}

const { title } = Astro.props.frontmatter as Props['frontmatter']
---

<BaseLayout title={title}>
  <article class='resume'>
    <slot />
  </article>
</BaseLayout>
